    "rimraf": "2.6.3",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "sortabular": "1.6.0",
    "source-map-loader": "0.2.4",
    "start-server-and-test": "^1.9.1",
    "style-loader": "0.23.1",
    "table-resolver": "4.1.1",
    "ts-jest": "24.0.1",
    "ts-loader": "5.3.3",
    "tslint": "5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.4000",
    "uuid": "^3.3.2",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "dependencies": {
    "@emotion/core": "10.0.10",
    "@patternfly/react-core": "3.16.4",
    "@patternfly/react-table": "2.5.11",
    "@rebass/emotion": "3.1.0",
    "@types/react": "16.8.17",
    "axios": "0.19.0",
    "client-oauth2": "4.2.3",
