    "emotion": "10.0.9",
    "emotion-theming": "10.0.9",
    "history": "4.9.0",
    "jsonwebtoken": "8.5.1",
    "match-sorter": "2.3.0",
    "moment": "^2.24.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-redux": "6.0.1",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
